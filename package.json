{
  "name": "@ostanlabs/md-retriever",
  "version": "0.1.0",
  "description": "Markdown-native hybrid search retrieval engine for Obsidian vaults",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsc -b",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:integration": "MSRL_MODEL_PATH=$HOME/.msrl/models/bge-m3 vitest run --reporter=verbose",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src/",
    "download-model": "tsx scripts/download-model.ts"
  },
  "dependencies": {
    "@xenova/transformers": "^2.17.0",
    "better-sqlite3": "^11.7.0",
    "chokidar": "^4.0.0",
    "faiss-node": "^0.5.1",
    "onnxruntime-node": "^1.17.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.0",
    "@types/node": "^20.0.0",
    "tsx": "^4.0.0",
    "typescript": "^5.5.0",
    "vitest": "^2.0.0"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ostanlabs/md_retriever.git"
  },
  "keywords": [
    "markdown",
    "search",
    "retrieval",
    "obsidian",
    "vector",
    "embeddings",
    "faiss",
    "hybrid-search",
    "bm25",
    "semantic-search"
  ],
  "author": "Ostan Labs",
  "license": "MIT"
}

